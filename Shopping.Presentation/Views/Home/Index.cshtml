    <script>
        document.addEventListener('DOMContentLoaded', function () {
            showLoaderForSwiper(true);

            setTimeout(() => {
                showLoaderForSwiper(false);
            }, 2000);

            function showLoaderForSwiper(show) {
                const stages = document.querySelectorAll('.Owl_Stage');

                stages.forEach(stage => {
                    const loader = stage.querySelector('.cube-loader');
                    const swiper = stage.querySelector('.swiper');

                    if (loader && swiper) {
                        if (show) {
                            loader.style.display = 'block';
                            swiper.style.display = 'none';
                        } else {
                            loader.style.display = 'none';
                            swiper.style.display = 'block';
                        }
                    } else {
                        console.error('Loader or swiper element not found in Owl_Stage:', stage);
                    }
                });
            }
        });
    </script>

