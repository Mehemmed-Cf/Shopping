    <style>
        html,
        body {
            position: relative;
            height: 100%;
        }

        .Owl_Outer {
            position: relative;
            overflow: hidden;
            height: 110%;
            margin-bottom: 150px;
            width: 1170px;
        }

        @@media (max-width: 991px) {
            .Owl_Outer {
                width: 750px;
            }
        }

/*         @@media (max-width: 762px) {
            .Owl_Outer {
                width: 550px;
            }
        } */

        .Owl_Stage {
            display: none;
            justify-content: center !important;
            align-items: center;
            opacity: 0;
            visibility: hidden;
            transition: all 0.5s ease-in-out;
            will-change: transform;
            height: 70%;
            width: 100%;
            position: relative;
        }

        .Active-Stage {
          display: flex !important;
          opacity: 1 !important;
          visibility: visible !important;
        }

        .Owl_Stage @@media (max-width: 991px) {
            width: 750px;
        }

        #Owl_Item {
            visibility: visible;
            width: 227.6px;
            padding: 16px 15px;
            float: left;
        }

        #Owl_Item .Single-Product {
            display: flex;
            justify-content: center;
            align-items: center;
            height: 289px;
            position: relative;
            background: #fff;
            transition: all 0.5s ease-in-out;
            flex-direction: column;
            cursor: pointer;
            will-change: transform;
        }

        #Owl_Item .Single-Product .Product-Img img {
            width: 200px;
        }

        #Owl_Item .Single-Product .Product-Content {
            transition: all 0.3s ease-in-out;
            z-index: 19;
        }

        #Owl_Item .Single-Product .Product-Content .ComeFromBottomAnimation {
            animation-name: ComeFromBottom;
            animation-duration: 2s;
            animation-timing-function: ease-in-out;
            animation-fill-mode: forwards;
        }

        #Owl_Item .Single-Product .Product-Content #popup_cart_title {
            font-family: sans-serif;
            font-size: 16px;
            font-weight: 500;
            line-height: 120%;
            color: var(--Background-Dark);
        }

        #Owl_Item .Single-Product .Product-Content #product-rating {
            display: flex;
        }

        #Owl_Item .Single-Product .Product-Content p {
            display: flex;
            flex-direction: column;
            padding-top: 40px;
            gap: 5px;
            color: var(--Font-Color);
        }

        #Owl_Item .Single-Product .Product-Content p #price {
            font-family: sans-serif;
            font-size: 21px;
            font-weight: 700;
            line-height: 120%;
        }

        #Owl_Item .Single-Product .Product-Content p #prev_price {
            color: #a4a4a4;
            font-family: sans-serif;
            font-size: 14px;
            font-weight: 400;
            line-height: 120%;
        }

        #Owl_Item .Single-Product .Product-Content #product-actions {
            display: flex;
            flex-wrap: nowrap;
            transition: 0.3s ease-in-out;
            position: absolute;
            width: 100%;
            z-index: 25;
        }

        #Owl_Item .Single-Product .Product-Content #product-actions .Primary-Actions {
            display: flex;
            justify-content: center;
            align-items: center;
            bottom: 1px;
            z-index: 30;
            left: -10px;
            position: absolute;
        }

        #Owl_Item .Single-Product .Product-Content #product-actions .Appear {
            opacity: 1 !important;
            visibility: visible !important;
            transition: opacity 0.6s ease !important;
        }

        #Owl_Item .Single-Product .Product-Content #product-actions .Secondary-Actions {
            display: flex;
            justify-content: center;
            align-items: center;
            visibility: hidden;
            bottom: 1px;
            right: -20px;
            position: absolute;
            z-index: 30;
            gap: 7px;
        }

        #Owl_Item .Single-Product .Product-Content #product-actions .Secondary-Actions #add-to-wishlist {
            display: flex;
            justify-content: center;
            align-items: center;
            background-color: var(--Background-Dark);
            height: 32px;
            width: 30px;
            border-radius: 5px;
            will-change: color;
            transition: opacity 0.3s ease;
            opacity: 0;
        }

        #Owl_Item .Single-Product .Product-Content #product-actions .Secondary-Actions #add-to-wishlist:hover {
            background-color: var(--Main-Blue);
        }

        #Owl_Item .Single-Product .Product-Content #product-actions .Secondary-Actions #add-to-wishlist img {
            width: 15px;
        }

        #Owl_Item .Single-Product .Product-Content #product-actions .Secondary-Actions #quickview {
            display: flex;
            justify-content: center;
            align-items: center;
            background-color: var(--Background-Dark);
            height: 32px;
            width: 30px;
            border-radius: 5px;
            will-change: color;
            transition: opacity 0.3s ease;
            opacity: 0;
        }

        #Owl_Item .Single-Product .Product-Content #product-actions .Secondary-Actions #quickview:hover {
            background-color: var(--Main-Blue);
        }

        #Owl_Item .Single-Product .Product-Content #product-actions .Secondary-Actions #quickview img {
            width: 15px;
        }

        .swiper {
            width: 100%;
            height: 100%;
        }

        .swiper-slide {
            text-align: center;
            vertical-align: middle;
            font-size: 18px;
            background: #fff;
            display: flex;
            justify-content: center;
            align-items: center;
        }

        .swiper-slide img {
            display: block;
            width: 100%;
            height: 100%;
        }

    </style>

    <style>

    .Header-Row {
      margin-right: -15px;
      margin-left: -15px;
      width: 100%;
