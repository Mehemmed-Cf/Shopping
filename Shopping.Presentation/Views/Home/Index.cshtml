    <script>
            document.addEventListener('DOMContentLoaded', function () {
                const emailInput = document.getElementById('Email-Subscribe');
                const submitButton = document.getElementById('submit-subscription');
                const errorMessage = document.getElementById('subscribe-error-message');

                submitButton.addEventListener('click', async function (event) {
                    event.preventDefault();

                    const email = emailInput.value.trim();
                    const response = await addSubscriber(email);

                    if (response.error) {
                        console.error(response.error);
                        errorMessage.textContent = 'Subscription failed. Please try again later.';
                    } else {
                        alert('Subscription successful');
                        errorMessage.textContent = '';
                    }
                });
            });

            async function checkEmailExists(email) {
                try {

                const response = await fetch(`/Admin/Subscribers/${email}`, {
                    method: 'GET',
                    headers: {
                        'X-Requested-With': 'XMLHttpRequest'
                    }
                });

                    if (!response.ok) {
                        throw new Error('Server responded with error: ' + response.status);
                    }
                    const data = await response.json();
                    return data.exists; // Assuming your backend returns { exists: true/false }
                } catch (error) {
                    console.error('Error checking email:', error.message);
                    return false; // Return false in case of any error
                }
            }

        async function addSubscriber(email) {
                try {
                const emailExists = await checkEmailExists(email);

                    if (emailExists) {
                        console.log('Email already exists.');
                        return { message: 'Email already subscribed' };
                    }

                const response = await fetch(`/Admin/Subscribers/Create`, {
                        method: 'POST',
                        headers: {
                        'Accept': 'application/json',
                        'X-Requested-With': 'XMLHttpRequest'
                        },
                    body: JSON.stringify({ email })
                    });

                    if (!response.ok) {
                        throw new Error('Failed to subscribe.');
                    }

                    return await response.json();
                } catch (error) {
                    console.error('Subscription error:', error.message);
                    return { error: 'Subscription failed. Please try again later.' };
                }
            }
    </script>

