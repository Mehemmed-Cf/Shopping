      font-size: 14px;
      font-weight: 600;
      line-height: 120%;
      color: var(--Background-Dark);
      position: absolute;
      left: 30px;
    }

    @@media (max-width: 991px) {
      .Header-Row .Product-List h2 {
        font-size: 13px;
      }
    }

    @@media (max-width: 762px) {
      .Header-Row .Product-List h2 {
        position: initial;
        font-size: 16px;
        margin-bottom: 20px;
        margin-right: 270px;
      }
    }

    .Header-Row .Product-List li {
      cursor: pointer;
      display: inline-block;
      font-family: sans-serif;
      font-size: 14px;
      font-weight: 600;
      line-height: 120%;
      padding-right: 40px;
      position: relative;
      text-transform: uppercase;
      will-change: color;
      white-space: nowrap;
      transition: 0.5s;
    }

    .Header-Row .Product-List li a {
      color: #aaa;
    }

    .Header-Row .Product-List .Active-List {
      transition: color 0.5s ease-in-out;
    }

    .Header-Row .Product-List .Active-List a {
